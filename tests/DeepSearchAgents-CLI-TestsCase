━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━ Step 6 ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
 ─ Executing parsed code: ──────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────
  import json
  import re

  # 1. Retrieve average Class-C RV mpg
  mpg_results_json = search_links(query="average Class C RV fuel efficiency mpg", num_results=3)
  results = json.loads(mpg_results_json)
  mpg = None
  for item in results:
      sn = item.get("snippet", "")
      m = re.search(r"(\d+(?:\.\d+)?)\s*mpg", sn)
      if m:
          mpg = float(m.group(1))
          break
  if mpg is None:
      mpg = 10.0  # default estimate
  print("mpg:", mpg)

  # 2. Constants for budgeting
  speed_mph = 55                       # avg driving speed
  gas_price = 3.153                   # USD per gallon (AAA national avg)
  nightly_rate = 40                   # USD per night at RV park
  food_cost_pppd = 30                 # USD per person per day

  # 3. Prepare calculations
  total_distance = 0
  total_driving_time = 0
  total_fuel_cost = 0
  total_lodging_cost = nightly_rate * 7
  total_food_cost = food_cost_pppd * 2 * 7

  itinerary = []
  for idx, seg in enumerate(segments, start=1):
      dist = seg["distance_miles"]
      # use provided duration if available, else estimate
      dur = seg.get("duration_hours")
      if dur is None:
          dur = round(dist / speed_mph, 2)
      fuel_cost = round((dist / mpg) * gas_price, 2)
      day_food_cost = food_cost_pppd * 2

      total_distance += dist
      total_driving_time += dur
      total_fuel_cost += fuel_cost

      itinerary.append({
          "day": idx,
          "origin": seg["origin"],
          "destination": seg["destination"],
          "distance_miles": dist,
          "duration_hours": dur,
          "fuel_cost": fuel_cost,
          "lodging_cost": nightly_rate,
          "food_cost": day_food_cost
      })

  totals = {
      "total_distance_miles": round(total_distance, 1),
      "total_driving_hours": round(total_driving_time, 2),
      "fuel_cost_total": round(total_fuel_cost, 2),
      "lodging_cost_total": total_lodging_cost,
      "food_cost_total": total_food_cost,
      "grand_total": round(total_fuel_cost + total_lodging_cost + total_food_cost, 2)
  }

  # Output for final assembly
  print(json.dumps({"itinerary": itinerary, "totals": totals}, indent=2))
