 ─ Executing parsed code: ──────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────
  import json
  import re

  # 1) Define the 7 daily segments by major waypoint pairs
  pairs = [
      ("New York, NY", "Cleveland, OH"),
      ("Cleveland, OH", "Chicago, IL"),
      ("Chicago, IL", "Omaha, NE"),
      ("Omaha, NE", "Denver, CO"),
      ("Denver, CO", "Moab, UT"),
      ("Moab, UT", "Las Vegas, NV"),
      ("Las Vegas, NV", "Los Angeles, CA")
  ]

  segments = []
  for origin, dest in pairs:
      query = f"driving distance from {origin} to {dest}"
      results_json = search_links(query=query, num_results=3)
      results = json.loads(results_json)

      # find first snippet that contains "mi"
      snippet = ""
      for item in results:
          sn = item.get("snippet", "")
          if "mi" in sn:
              snippet = sn
              break

      # parse distance in miles and duration in hours
      dist = None
      dur = None
      if snippet:
          m1 = re.search(r"([\d,]+(?:\.\d+)?)\s*mi", snippet)
          m2 = re.search(r"approx\.?\s*(\d+)h\s*(\d+)min", snippet)
          if not m2:
              m2 = re.search(r"(\d+)h\s*(\d+)min", snippet)
          if m1:
              dist = float(m1.group(1).replace(",", ""))
          if m2:
              dur = int(m2.group(1)) + int(m2.group(2)) / 60

      segments.append({
          "origin": origin,
          "destination": dest,
          "distance_miles": round(dist, 1) if dist else None,
          "duration_hours": round(dur, 2) if dur else None
      })

  # Output the parsed segments for the next step
  print(json.dumps(segments, indent=2))
